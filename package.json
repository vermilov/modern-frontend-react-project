{
  "name": "modern-frontend-project",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --config ./build-utils/webpack.config.js --env.env=dev",
    "dev": "webpack --config ./build-utils/webpack.config.js --env.env=dev",
    "yarn": "webpack --config ./build-utils/webpack.config.js --env.env=prod",
    "buildanalyze": "yarn prod --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "test": "jest --config ./jest.config.json",
    "test:watch": "yarn test --watchAll",
    "test:cypress": "start-server-and-test start http://localhost:8080 cypress",
    "cypress": "cypress run"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cypress": "^3.4.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.10",
    "react-test-renderer": "^16.9.0",
    "start-server-and-test": "^1.10.0",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "peerDependencies": {
    "eslint-config-airbnb": "^17.1.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "global": "^4.4.0",
    "http-server": "^0.11.1",
    "uuid": "^3.3.2"
  }
